{
    "functions": {
        "api/**/*.php": {
            "runtime": "vercel-php@0.7.5"
        }
    },
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "src": "/sw.js",
            "dest": "/sw.js"
        },
        {
            "src": "/admin/(.*)",
            "dest": "/api/admin/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/$1"
        }
    ]
}